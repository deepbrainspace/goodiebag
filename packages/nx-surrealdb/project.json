{
  "name": "nx-surrealdb",
  "sourceRoot": "packages/nx-surrealdb/src",
  "projectType": "library",
  "targets": {
    "install": {
      "executor": "nx:run-commands",
      "outputs": [],
      "cache": false,
      "options": {
        "commands": [
          {
            "command": "pnpm install --frozen-lockfile",
            "forwardAllArgs": false
          }
        ],
        "cwd": "packages/nx-surrealdb",
        "parallel": false
      }
    },
    "build-ts": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["install"],
      "cache": false,
      "options": {
        "outputPath": "{workspaceRoot}/packages/nx-surrealdb/dist",
        "main": "{workspaceRoot}/packages/nx-surrealdb/src/index.ts",
        "tsConfig": "{workspaceRoot}/packages/nx-surrealdb/tsconfig.lib.json",
        "assets": [
          {
            "input": "packages/nx-surrealdb",
            "glob": "{package.json,generators.json,executors.json,README.md,LICENSE.md,ARCHITECTURE.md}",
            "output": "./"
          },
          {
            "input": "packages/nx-surrealdb/src",
            "glob": "**/schema.json",
            "output": "src"
          },
          {
            "input": "packages/nx-surrealdb/src/schema",
            "glob": "**/*",
            "output": "src/schema"
          }
        ]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-ts"],
      "options": {
        "commands": [
          {
            "command": "cp README.md ARCHITECTURE.md executors.json generators.json package.json pnpm-lock.yaml dist/",
            "forwardAllArgs": false
          },
          {
            "command": "cp -r src/generators/init/files dist/src/generators/init/",
            "forwardAllArgs": false
          },
          {
            "command": "cp -r src/generators/migration/files dist/src/generators/migration/",
            "forwardAllArgs": false
          }
        ],
        "cwd": "packages/nx-surrealdb",
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/nx-surrealdb/src/**/*.ts", "packages/nx-surrealdb/src/**/*.js"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "cache": false,
      "options": {
        "jestConfig": "packages/nx-surrealdb/jest.config.ts"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "pnpm publish --no-git-checks"
      }
    },
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm version {args.type}"
      },
      "configurations": {
        "patch": {
          "command": "pnpm version patch"
        },
        "minor": {
          "command": "pnpm version minor"
        },
        "major": {
          "command": "pnpm version major"
        },
        "prerelease": {
          "command": "pnpm version prerelease"
        }
      }
    }
  }
}