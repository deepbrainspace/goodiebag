{
  "generators": {
    "init": {
      "factory": "./src/generators/init/generator.js",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize a new SurrealDB database project with module structure"
    },
    "migration": {
      "factory": "./src/generators/migration/generator.js",
      "schema": "./src/generators/migration/schema.json",
      "description": "Generate a new SurrealDB migration"
    },
    "export-module": {
      "factory": "./src/generators/export-module/generator.js",
      "schema": "./src/generators/export-module/schema.json",
      "description": "Export a migration module as a reusable package"
    },
    "import-module": {
      "factory": "./src/generators/import-module/generator.js",
      "schema": "./src/generators/import-module/schema.json",
      "description": "Import a migration module package into the current project"
    }
  }
}
